<%- | String $config_name,
      Optional[String] $account,
      Optional[Sensitive] $secret_key,
      Optional[Sensitive] $sas_url,
| -%>
# Puppet managed file
[<%= $config_name %>]
type = azureblob
<% if $account { -%>
account = <%= $account %>
<% } -%>
<% if $secret_key { - %>
key = <%= $secret_key %>
<% } -%>
<% if $sas_url { - %>
sas_url = <%= $sas_url %>
<% } -%>
